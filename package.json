{
  "name": "express-sandbox",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run clean && cp -r src dist",
    "clean": "rimraf dist",
    "dev": "nodemon",
    "env:dev": "NODE_ENV=development",
    "env:prod": "NODE_END=production",
    "serve:dev": "npm run env:dev && npm run serve",
    "serve:prod": "npm run env:prod && npm run serve",
    "serve": "npm run build && npm run start",
    "start": "node ./dist/bin/www",
    "test": "jest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2"
  },
  "nodemonConfig": {
    "exec": "npm run serve:dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
